<config>
    <var name="name">
        <string>Hero</string>
    </var>
    <var name="level">
        <expr op="+">
            <number>5</number>
            <number>3</number>
        </expr>
    </var>
    <var name="inventory">
        <array>
            <string>Sword</string>
            <string>Shield</string>
            <string>Potion</string>
        </array>
    </var>
    <var name="health">
        <expr op="*">
            <number>100</number>
            <expr op="-">
                <number>10</number>
                <number>2</number>
            </expr>
        </expr>
    </var>
</config>